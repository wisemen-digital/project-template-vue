    "i18n-iso-countries": "7.13.0",
    "jwt-encode": "1.0.1",
    "libphonenumber-js": "1.11.16",
    "motion-v": "^0.5.0",
    "pinia": "2.3.0",
    "reka-ui": "1.0.0-alpha.8",
    "tailwind-merge": "2.5.5",
    "vue": "3.5.13",
    "vue-i18n": "10.0.5",
    "vue-router": "4.5.0",
    "zod": "3.24.1"
