    "tailwind-merge": "2.5.5",
    "vue": "3.5.13",
    "vue-i18n": "10.0.5",
    "vue-router": "4.5.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "0.59.0",
