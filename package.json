{
  "type": "module",
  "packageManager": "pnpm@9.6.0",
  "scripts": {
    "dev": "vite",
    "development": "vue-tsc --noEmit && vite build --mode development",
    "staging": "vue-tsc --noEmit && vite build --mode staging",
    "production": "vue-tsc --noEmit && vite build --mode production",
    "build": "vite build",
    "preview": "import-meta-env -x .env.e2e && vite preview",
    "lint:fix": "eslint . --fix --cache",
    "lint": "eslint . --cache",
    "openapi-ts": "openapi-ts -f openapi.config.ts",
    "test:unit": "vitest --run",
    "test:e2e": "npx playwright test",
    "prepare": "husky install",
    "type-check": "vue-tsc --build --force",
    "precommit": "pnpm type-check && pnpm lint && pnpm test:unit && pnpm test:e2e && pnpm build"
  },
