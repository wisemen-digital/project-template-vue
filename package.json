    "@tanstack/vue-query": "5.62.2",
    "@vueuse/core": "12.0.0",
    "@vueuse/router": "12.0.0",
    "@wisemen/vue-core": "1.0.0-alpha.3",
    "@wisemen/vue-core-auth": "1.1.0",
    "axios": "1.7.7",
    "clsx": "2.1.1",
    "cva": "1.0.0-beta.1",
    "dayjs": "1.11.13",
    "formango": "2.0.38",
