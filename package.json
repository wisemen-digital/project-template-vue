{
  "type": "module",
  "packageManager": "pnpm@9.6.0",
  "scripts": {
    "dev": "pnpm project-setup && vite",
    "development": "vue-tsc --noEmit && vite build --mode development",
    "staging": "vue-tsc --noEmit && vite build --mode staging",
    "production": "vue-tsc --noEmit && vite build --mode production",
    "build": "vite build",
    "preview": "import-meta-env -x .env.e2e && vite preview",
    "lint:fix": "eslint . --fix --cache",
    "lint": "eslint . --cache",
    "test:unit": "vitest --run",
    "test:e2e": "npx playwright test",
    "prepare": "husky install",
    "type-check": "vue-tsc --build --force",
    "precommit": "pnpm type-check && pnpm lint && pnpm test:unit && pnpm test:e2e && pnpm build",
    "project-setup": "npx tsx setup.ts"
  },
  "dependencies": {
    "@appwise/zod-http-client": "0.0.6",
    "@fontsource-variable/inter": "5.1.0",
    "@sentry/integrations": "7.114.0",
    "@sentry/tracing": "7.114.0",
    "@sentry/vue": "8.32.0",
    "@tailwindcss/container-queries": "0.1.1",
    "@tanstack/vue-query": "5.56.2",
    "@vueuse/core": "11.1.0",
    "@vueuse/router": "11.1.0",
    "@wisemen/oauth2-vue-client": "2.1.0",
    "@wisemen/vue-core": "1.0.0-alpha.10",
    "axios": "1.7.7",
    "clsx": "2.1.1",
    "cva": "1.0.0-beta.1",
    "dayjs": "1.11.13",
    "formango": "2.0.35",
    "i18n-iso-countries": "^7.13.0",
    "libphonenumber-js": "1.11.9",
    "pinia": "2.2.0",
    "radix-vue": "1.9.6",
    "tailwind-merge": "2.5.2",
    "vue": "3.5.12",
    "vue-i18n": "9.14.1",
    "vue-router": "4.4.5",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@import-meta-env/cli": "0.6.9",
    "@import-meta-env/unplugin": "0.5.2",
    "@playwright/test": "1.45.3",
    "@tanstack/vue-query-devtools": "5.51.21",
    "@tsconfig/node20": "20.1.4",
    "@types/jsdom": "21.1.7",
    "@types/node": "22.1.0",
    "@vitejs/plugin-vue": "5.1.4",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.5.1",
    "@wisemen/eslint-config-vue": "0.1.5",
    "@wisemen/vue-core-query": "0.0.31",
    "autoprefixer": "10.4.20",
    "chalk": "^5.3.0",
    "dotenv": "16.4.5",
    "eslint": "9.8.0",
    "eslint-plugin-playwright": "1.6.2",
    "fs": "0.0.1-security",
    "fs-extra": "^11.2.0",
    "husky": "9.1.6",
    "inquirer": "^12.1.0",
    "jsdom": "24.1.1",
    "postcss": "8.4.47",
    "sass": "1.79.3",
    "tailwindcss": "3.4.13",
    "tsx": "^4.19.2",
    "typescript": "5.5.4",
    "vite": "5.3.5",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-pwa": "0.20.5",
